<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="458dp"
        android:layout_height="54dp"
        android:background="@color/white"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/botomm_navigation" />

    <TextView
        android:id="@+id/welcomeTXT"
        android:layout_width="292dp"
        android:layout_height="49dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:fontFamily="sans-serif-black"
        android:foregroundTint="#2196F3"
        android:text="Bienvenido, Ricardo"
        android:textColor="#2196F3"
        android:textColorHighlight="#2196F3"
        android:textColorHint="#03A9F4"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="209dp"
        android:layout_height="209dp"

        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toTopOf="@+id/bottom_navigation"
        app:layout_constraintEnd_toEndOf="@+id/imageView27"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView27"
        app:layout_constraintTop_toBottomOf="@+id/imageView27"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@mipmap/ic_center_doc_foreground" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="380dp"
        android:layout_height="126dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_logo_foreground" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginBottom="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_emergencia_foreground" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginStart="105dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_ambu_service_foreground" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_bene_foreground" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginTop="126dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_medico_foreground" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="104dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_segui_foreground" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="209dp"
        android:layout_height="209dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:srcCompat="@mipmap/ic_aesosria_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>